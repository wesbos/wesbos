  "homepage": "https://wesbos.com",
  "keywords": [
    "wesbos"
  ],
  "eslintConfigxx": {
    "extends": [
      "wesbos/typescript"
    ],
    "rules": {
      "react/prop-types": 0,
      "import/no-unresolved": 0,
      "//Need to turn this off while we upgrade to ESLint 9. Something weird with the css module approach": 0,
      "@typescript-eslint/no-unsafe-assignment": 0
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "dev": "next dev --turbo",
    "build": "next build",
    "build:mdx-index": "node --experimental-strip-types ./lib/createMDXIndex.ts",
    "build:worker": "opennextjs-cloudflare",
    "start": "next start",
    "dev:worker": "wrangler dev --port 8771",
    "dev:worker:remote": "wrangler dev --port 8771 --remote",
    "preview:worker": "pnpm run build:worker && pnpm run dev:worker",
    "deploy:worker": "pnpm run build:worker && wrangler deploy",
    "types:worker": "wrangler types --experimental-include-runtime  --env-interface CloudflareEnv",
    "types:css": "node --experimental-strip-types ./lib/createStyleIndex.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "tip": "tsx --conditions=react-server ./createTip.ts"
  },
  "packageManager": "pnpm@9.10.0+sha1.216899f511c8dfde183c7cb50b69009c779534a8",
  "pnpm": {
    "overrides": {
      "axios": "1.7.9"
    },
    "patchedDependencies": {
      "rehype-extract-excerpt": "patches/rehype-extract-excerpt.patch"
    }
  }
}
